<ul>
  {%- for post in collections.posts | reverse -%}
    <li class="post-list">
      <span><code>{{ post.date | dateFormat }}</code></span>
      <span><a href="{{ post.url }}">{{ post.data.title }}</a></span>
      <p class="post-description">{{ post.data.description }}</p>
    </li>
  {%- endfor -%}
</ul>
